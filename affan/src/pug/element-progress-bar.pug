doctype html
html(lang="en")
    head
        include inc/head.pug

    body
        include inc/loader.pug

        include inc/internet-status.pug

        include inc/dark-rtl-loader.pug

        include inc/setting-popup.pug

        // Header Area 
        .header-area#headerArea
            .container
                // Header Content 
                .header-content.position-relative.d-flex.align-items-center.justify-content-between
                    // Back Button 
                    .back-button
                        a(href="elements.html")
                            include inc/back-btn.pug

                    // Page Title 
                    .page-heading
                        h6.mb-0 Progress Bar

                    // Settings 
                    .setting-wrapper
                        include inc/setting-btn.pug

        .page-content-wrapper.py-3
            .container
                // Element Heading 
                .element-heading
                    h6 Bootstrap Progress Bar

            .container
                .card
                    .card-body
                        // Default Progress 
                        .progress
                            .progress-bar.w-25(role="progressbar", aria-valuenow="25", aria-valuemin="0", aria-valuemax="100") 25%

                        .mb-3

                        // Success Progress 
                        .progress
                            .progress-bar.bg-success.w-50(role="progressbar", aria-valuenow="50", aria-valuemin="0", aria-valuemax="100") 50%

                        .mb-3

                        // Danger Progress 
                        .progress
                            .progress-bar.bg-danger.w-75(role="progressbar", aria-valuenow="75", aria-valuemin="0", aria-valuemax="100") 75%

                        .mb-3

                        // Warning Progress 
                        .progress
                            .progress-bar.bg-warning.w-100(role="progressbar", aria-valuenow="100", aria-valuemin="0", aria-valuemax="100") 100%

            .container
                // Element Heading 
                .element-heading.mt-3
                    h6 Skill Progress Bar

            .container
                .card
                    .card-body
                        // Single Skill Progress Bar 
                        .skill-progress-bar.d-flex.align-items-center
                            // Skill Icon 
                            .skill-icon.shadow-sm
                                svg.bi.bi-code.text-dark(width="24" height="24" viewBox="0 0 16 16" fill="currentColor" xmlns="http://www.w3.org/2000/svg")
                                    path(fill-rule="evenodd" d="M5.854 4.146a.5.5 0 0 1 0 .708L2.707 8l3.147 3.146a.5.5 0 0 1-.708.708l-3.5-3.5a.5.5 0 0 1 0-.708l3.5-3.5a.5.5 0 0 1 .708 0zm4.292 0a.5.5 0 0 0 0 .708L13.293 8l-3.147 3.146a.5.5 0 0 0 .708.708l3.5-3.5a.5.5 0 0 0 0-.708l-3.5-3.5a.5.5 0 0 0-.708 0z")

                            // Skill Data 
                            .skill-data
                                // Skill Name
                                .skill-name.d-flex.align-items-center.justify-content-between
                                    p.mb-1 HTML5
                                    small.mb-1
                                        span 78
                                        | %
                                // Progress 
                                .progress(style="height: 4px;")
                                    .progress-bar(style="width: 78%;", role="progressbar", aria-valuenow="78", aria-valuemin="0", aria-valuemax="100")


                        // Single Skill Progress Bar 
                        .skill-progress-bar.d-flex.align-items-center
                            // Skill Icon 
                            .skill-icon.shadow-sm
                                svg.bi.bi-heart.text-danger(width="20" height="20" viewBox="0 0 16 16" fill="currentColor" xmlns="http://www.w3.org/2000/svg")
                                    path(fill-rule="evenodd" d="M8 2.748l-.717-.737C5.6.281 2.514.878 1.4 3.053c-.523 1.023-.641 2.5.314 4.385.92 1.815 2.834 3.989 6.286 6.357 3.452-2.368 5.365-4.542 6.286-6.357.955-1.886.838-3.362.314-4.385C13.486.878 10.4.28 8.717 2.01L8 2.748zM8 15C-7.333 4.868 3.279-3.04 7.824 1.143c.06.055.119.112.176.171a3.12 3.12 0 0 1 .176-.17C12.72-3.042 23.333 4.867 8 15z")

                            // Skill Data 
                            .skill-data
                                // Skill Name 
                                .skill-name.d-flex.align-items-center.justify-content-between
                                    p.mb-1 PHP 8
                                    small.mb-1
                                        span 96
                                        | %
                                // Progress 
                                .progress(style="height: 4px;")
                                    .progress-bar.bg-success(style="width: 96%;", role="progressbar", aria-valuenow="96", aria-valuemin="0", aria-valuemax="100")


                        // Single Skill Progress Bar 
                        .skill-progress-bar.d-flex.align-items-center
                            // Skill Icon 
                            .skill-icon.shadow-sm
                                svg.bi.bi-bootstrap.text-primary(width="24" height="24" viewBox="0 0 16 16" fill="currentColor" xmlns="http://www.w3.org/2000/svg")
                                    path(fill-rule="evenodd" d="M12 1H4a3 3 0 0 0-3 3v8a3 3 0 0 0 3 3h8a3 3 0 0 0 3-3V4a3 3 0 0 0-3-3zM4 0a4 4 0 0 0-4 4v8a4 4 0 0 0 4 4h8a4 4 0 0 0 4-4V4a4 4 0 0 0-4-4H4z")
                                    path(fill-rule="evenodd" d="M8.537 12H5.062V3.545h3.399c1.587 0 2.543.809 2.543 2.11 0 .884-.65 1.675-1.483 1.816v.1c1.143.117 1.904.931 1.904 2.033 0 1.488-1.084 2.396-2.888 2.396zM6.375 4.658v2.467h1.558c1.16 0 1.764-.428 1.764-1.23 0-.78-.569-1.237-1.541-1.237H6.375zm1.898 6.229H6.375V8.162h1.822c1.236 0 1.887.463 1.887 1.348 0 .896-.627 1.377-1.811 1.377z")

                            // Skill Data 
                            .skill-data
                                // Skill Name 
                                .skill-name.d-flex.align-items-center.justify-content-between
                                    p.mb-1 Bootstrap 5
                                    small.mb-1
                                        span 88
                                        | %
                                // Progress 
                                .progress(style="height: 4px;")
                                    .progress-bar.bg-info(style="width: 88%;", role="progressbar", aria-valuenow="88", aria-valuemin="0", aria-valuemax="100")

            .container
                // Element Heading 
                .element-heading.mt-3
                    h6 Upload Data Progress

            .container
                .card
                    .card-body
                        h6 Uploading...

                        // Progress 
                        .progress(style="height: 6px;")
                            .progress-bar.progress-bar-striped.progress-bar-animated(style="width: 93%;", role="progressbar", aria-valuenow="93", aria-valuemin="0", aria-valuemax="100")

                        // Progress Info 
                        .progress-info.d-flex.align-items-center.justify-content-between
                            span 93%
                            span 17 sec remaining

            .container
                // Element Heading 
                .element-heading.mt-3
                    h6 Task Progress

            .container
                .card
                    .card-body
                        // Single Task Progress 
                        .single-task-progress
                            // Progress Info 
                            .progress-info.d-flex.align-items-center.justify-content-between
                                h6.mb-1 Project Affan
                                span.mt-0.mb-1 70% done

                            // Progress 
                            .progress(style="height: 4px;")
                                .progress-bar.bg-danger(style="width: 70%;", role="progressbar", aria-valuenow="70", aria-valuemin="0", aria-valuemax="100")

                            .task-member-info.d-flex.align-items-center.justify-content-between
                                // Who working 
                                .who-working.mt-2
                                    a(href="#")
                                        img.shadow-sm(src="img/bg-img/7.jpg", alt="")
                                    a(href="#")
                                        img.shadow-sm(src="img/bg-img/8.jpg", alt="")
                                    a(href="#")
                                        img.shadow-sm(src="img/bg-img/9.jpg", alt="")
                                
                                // Add New Member 
                                .addnew-member.mt-2
                                    a.btn.btn-sm.btn-danger.btn-round(href="#") Add New

                        // Single Task Progress 
                        .single-task-progress
                            // Progress Info 
                            .progress-info.d-flex.align-items-center.justify-content-between
                                h6.mb-1 Project Suha
                                span.mt-0.mb-1 93% done

                            // Progress 
                            .progress(style="height: 4px;")
                                .progress-bar.bg-success(style="width: 93%;", role="progressbar", aria-valuenow="93", aria-valuemin="0", aria-valuemax="100")

                            .task-member-info.d-flex.align-items-center.justify-content-between
                                // Who working 
                                .who-working.mt-2
                                    a(href="#")
                                        img.shadow-sm(src="img/bg-img/7.jpg", alt="")
                                    a(href="#")
                                        img.shadow-sm(src="img/bg-img/8.jpg", alt="")
                                    a(href="#")
                                        img.shadow-sm(src="img/bg-img/9.jpg", alt="")
                                    a(href="#")
                                        img.shadow-sm(src="img/bg-img/7.jpg", alt="")
                                    a(href="#")
                                        img.shadow-sm(src="img/bg-img/8.jpg", alt="")
                                
                                // Add New Member 
                                .addnew-member.mt-2
                                    a.btn.btn-sm.btn-success.btn-round(href="#") Add New

                        // Single Task Progress 
                        .single-task-progress
                            // Progress Info 
                            .progress-info.d-flex.align-items-center.justify-content-between
                                h6.mb-1 Project Saasbox
                                span.mt-0.mb-1 89% done

                            // Progress 
                            .progress(style="height: 4px;")
                                .progress-bar.bg-warning(style="width: 89%;", role="progressbar", aria-valuenow="89", aria-valuemin="0", aria-valuemax="100")

                            .task-member-info.d-flex.align-items-center.justify-content-between
                                // Who working 
                                .who-working.mt-2
                                    a(href="#")
                                        img.shadow-sm(src="img/bg-img/7.jpg", alt="")
                                    a(href="#")
                                        img.shadow-sm(src="img/bg-img/8.jpg", alt="")
                                
                                // Add New Member 
                                .addnew-member.mt-2
                                    a.btn.btn-sm.btn-warning.btn-round(href="#") Add New

        include inc/footer-nav.pug

        include inc/footer.pug